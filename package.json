{
  "name": "sse-utils",
  "description": "Utility functions for serializing and deserializing SSE messages",
  "version": "1.0.0",
  "author": "Wouter Dullaert",
  "homepage": "https://github.com/wdullaer/sse-utils",
  "bugs": "https://github.com/wdullaer/sse-utils/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdullaer/sse-utils.git"
  },
  "keywords": [
    "sse",
    "server sent events",
    "utils"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel lib --out-dir dist",
    "coverage": "npm run lint && ./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover --root lib/ ./node_modules/mocha/bin/_mocha",
    "lint": "./node_modules/.bin/eslint ./lib",
    "test": "npm run lint && ./node_modules/.bin/mocha",
    "prepublish": "npm run test && npm run build"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "dependencies": {
    "babel-runtime": "~6.11.6"
  },
  "devDependencies": {
    "babel-cli": "~6",
    "babel-plugin-transform-runtime": "~6.12.0",
    "babel-preset-es2015": "~6.13.2",
    "babel-register": "~6.11.6",
    "chai": "~3.5.0",
    "eslint": "~3.3.1",
    "eslint-config-standard": "~5.3.5",
    "eslint-plugin-standard": "~2.0.0",
    "eslint-plugin-promise": "~2.0.1",
    "isparta": "~4.0.0",
    "mocha": "~3.0.2"
  }
}
